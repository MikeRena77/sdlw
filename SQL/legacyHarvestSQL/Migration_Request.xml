  <dbcombobox cols="30" dbfield="migharpack" hsql="select packagename from harpackage" id="migharpack" label="Harvest Package No."/>

  <combobox cols="22" dbfield="migsrcenv" id="migsrcenv" label="Source Environment *" maxsize="22" rows="1">
